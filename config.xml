<?xml version="1.0" ?>
<config>  
	<!-- Global simulation parameters -->
	<output resultsFile="./data/province.h5" logsDir="./logs"/>
	<numSteps value="5000" serializeResolution="135"/> 
	<!-- be sure than serializResolution == X * cultural step * 3 -->
	<size width="1800" height="1"/>
	<numAgents value="300"/>
	<market size=".5"/>


	<controller type="basic" >
	    <good type="randn"/> <!-- how good need are initialized can be : random,randn,manual,gintis07--> 
	    <agent type="custom"/> <!--not used-->
	    <trade volsel="brughman17" util="brughman17"/>
	    <!-- trade/volsel allow to choose how requested quantities are selected can be : custom,gintis07,gintis06,gintis06-outneed -->
<!--trade/util allow to choose how agent score is computed can be : custom,gintis07,gintis06,brughman17-->

	    <culture transmission="copymin" innovation="trade" mumax="1" mutation=".18" copy="1" step="15"/> 

	</controller>

	<popdistrib type="pl" max="500000" min="2000" alpha="-2.23"/> <!--this allow to define fuction that willl define the distribution of the size of the agents-->
