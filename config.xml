<?xml version="1.0" ?>
<config>  
	<!-- Global simulation parameters -->
	<output resultsFile="./data/province.h5" logsDir="./logs"/>
	<numSteps value="1000" serializeResolution="69"/> 
	<!-- be sure than serializResolution == X * cultural step * 3 -->
	<size width="1800" height="1"/>
	<numAgents value="400"/>
	<market size=".5"/>
	<controller type="basic" >
		<good type="random"/>
		<culture transmission="trade" innovation="trade" mutation=".04" step="23"/>
	</controller>

	<!--
	<network type="sw" nparam="2">
	    	<prop0 id="v" value= "2"/>
	    	<prop1 id="p" value= ".1"/>
	</network>
	-->
	<!--
	<network type="sf" nparam="1">
	    	<prop0 id="m" value= "3"/>
	</network>
	-->
	<network type="full" nparam="1">
	    	<prop0 id="p" value= ".2"/>
	</network>

	<goods num="2">
		<good	id="proto"	initialQuantity="0"	maxQuantity="1000"	price="-1.0"	need="-1.0"	productionRate="0"/>
	</goods>

	<rasters num="3">
		<raster0	id="currency"	min="0.0"	max="20.0"	init="20.0"/>
		<raster1	id="ess-a"	min="0.0"	max="20.0"	init="20.0"/>
		<raster2	id="ess-b"	min="0.0"	max="20.0"	init="20.0"/>
		<raster3	id="nonEss-a"	min="0.0"	max="20.0"	init="20.0"/>
		<raster4	id="nonEss-b"	min="0.0"	max="20.0"	init="20.0"/>
	</rasters>
</config>

