<?xml version="1.0" ?>
<config>  
	<!-- Global simulation parameters -->
	<output resultsFile="./data/province.h5" logsDir="./logs"/>
	<numSteps value="15000" serializeResolution="225"/> 
	<!-- be sure than serializResolution == X * cultural step * 3 -->
	<size width="1800" height="5"/>
	<logs>
	    <trade value="false"/>
	</logs>
	<numAgents value="1000"/>
	<market size="1"/>
	<controller type="basic" >
	    <good type="manual"/> <!-- how good need are initialized can be : random,randn,manual,gintis07--> 
	    <agent type="custom"/> <!--not used-->
	    <trade volsel="brughman17" util="brughman17"/>
	    <!-- trade/volsel allow to choose how requested quantities are selected can be : custom,gintis07,gintis06,gintis06-outneed -->
<!--trade/util allow to choose how agent score is computed can be : custom,gintis07,gintis06,brughman17-->

	    <culture transmission="copymax" innovation="trade" mumax="1" mutation=".1" copy="1" step="15"/> 
	    
	   
	</controller>

	<popdistrib type="pl" max="500000" min="2000" alpha="-2.23" impact="off"/> <!--this allow to define fuction that willl define the distribution of the size of the agents-->
	<!--
	<network type="sw" nparam="2">
	    	<prop0 id="v" value= "2"/>
	    	<prop1 id="p" value= ".1"/>
	</network>
	-->
	<!--
	<network type="sf" nparam="1">
	    	<prop0 id="m" value= "3"/>
	</network>
	-->
	<network type="integrate" nparam="1"> <!-- type of network can be full or integrate  -->
	    	<prop0 id="p" value= ".2"/>
	</network>


	<!--
	<goods num="3">
	       	<good   id="proto"      initialQuantity="0"     maxQuantity="1000"      price="-1.0"    need="-1.0"     productionRate="0"/>
	</goods>
-->

	<goods num="3" prod="unbalanced">
		<good0	id="g0"	initialQuantity="0"	maxQuantity="1000"	price="-1"	need="1"	productionRate="0" nprod="5"/>
		<good1	id="g1"	initialQuantity="0"	maxQuantity="1000"	price="-1"	need="1"	productionRate="0" nprod="5"/>
		<good2	id="coins"	initialQuantity="0"	maxQuantity="1000"	price="1"	need="1"	productionRate="0" nprod="-1"/>
		<good3	id="coins"	initialQuantity="0"	maxQuantity="1000"	price="1"	need="1"	productionRate="0" nprod="-1"/>
		<good4	id="g4"	initialQuantity="0"	maxQuantity="1000"	price="-1"	need="1"	productionRate="0" nprod="14"/>
		<good5	id="coins"	initialQuantity="0"	maxQuantity="1000"	price="1"	need="1"	productionRate="0" nprod="-1"/>
		<good4	id="g4"	initialQuantity="0"	maxQuantity="1000"	price="1"	need="5.0"	productionRate="0"/>
		<good5	id="g5"	initialQuantity="0"	maxQuantity="1000"	price="1"	need="6.0"	productionRate="0"/>
	</goods>

	<events type="rate" rate="40" stop="7200"/> 


</config>

